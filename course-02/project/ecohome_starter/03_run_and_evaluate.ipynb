    "  },\n",
    "  {\n",
    "   \"cell_type\": \"markdown\",\n",
    "   \"id\": \"5760814c\",\n",
    "   \"metadata\": {},\n",
    "   \"source\": [\n",
    "    \"## 4. Evaluate Responses\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"id\": \"1da0640e\",\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# TODO: Implement evaluation functions\\n\",\n",
    "    \"# Create functions to evaluate:\\n\",\n",
    "    \"# - Final Response\\n\",\n",
    "    \"# - Tool usage\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"id\": \"031e858d\",\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# TODO: Create a response evaluator\\n\",\n",
    "    \"def evaluate_response(question, final_response, expected_response):\\n\",\n",
    "    \"    \\\"\\\"\\\"Evaluate a single response against expected response using LLM\\\"\\\"\\\"\\n\",\n",
    "    \"    from langchain_openai import ChatOpenAI\\n\",\n",
    "    \"    import os\\n\",\n",
    "    \"    from dotenv import load_dotenv\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    load_dotenv()\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    evaluator_llm = ChatOpenAI(\\n\",\n",
    "    \"        model=\\\"gpt-4o-mini\\\",\\n\",\n",
    "    \"        temperature=0.0,\\n\",\n",
    "    \"        base_url=\\\"https://openai.vocareum.com/v1\\\",\\n\",\n",
    "    \"        api_key=os.getenv(\\\"VOCAREUM_API_KEY\\\")\\n\",\n",
    "    \"    )\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    evaluation_prompt = f\\\"\\\"\\\"You are an expert evaluator for an energy advisor AI agent.\\n\",\n",
    "    \"    \\n\",\n",
    "    \"Evaluate the following response based on these criteria:\\n\",\n",
    "    \"1. **Accuracy** (0-10): Is the information correct and realistic?\\n\",\n",
    "    \"2. **Relevance** (0-10): Does it directly address the question?\\n\",\n",
    "    \"3. **Completeness** (0-10): Is the answer comprehensive with actionable advice?\\n\",\n",
    "    \"4. **Reasoning** (0-10): Is the logic clear and well-explained?\\n\",\n",
    "    \"\\n\",\n",
    "    \"Question: {question}\\n\",\n",
    "    \"\\n\",\n",
    "    \"Expected Response Criteria: {expected_response}\\n\",\n",
    "    \"\\n\",\n",
    "    \"Actual Response: {final_response}\\n\",\n",
    "    \"\\n\",\n",
    "    \"Provide your evaluation in this exact format:\\n\",\n",
    "    \"ACCURACY: [score]\\n\",\n",
    "    \"RELEVANCE: [score]\\n\",\n",
    "    \"COMPLETENESS: [score]\\n\",\n",
    "    \"REASONING: [score]\\n\",\n",
    "    \"OVERALL: [average of all scores]\\n\",\n",
    "    \"FEEDBACK: [brief feedback on strengths and weaknesses]\\n\",\n",
    "    \"\\\"\\\"\\\"\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    try:\\n\",\n",
    "    \"        result = evaluator_llm.invoke(evaluation_prompt)\\n\",\n",
    "    \"        eval_text = result.content\\n\",\n",
    "    \"        \\n\",\n",
    "    \"        # Parse scores from response\\n\",\n",
    "    \"        scores = {}\\n\",\n",
    "    \"        for line in eval_text.split('\\\\n'):\\n\",\n",
    "    \"            if ':' in line:\\n\",\n",
    "    \"                key, value = line.split(':', 1)\\n\",\n",
    "    \"                key = key.strip().upper()\\n\",\n",
    "    \"                if key in ['ACCURACY', 'RELEVANCE', 'COMPLETENESS', 'REASONING', 'OVERALL']:\\n\",\n",
    "    \"                    try:\\n\",\n",
    "    \"                        scores[key.lower()] = float(value.strip().split()[0])\\n\",\n",
    "    \"                    except:\\n\",\n",
    "    \"                        scores[key.lower()] = 5.0  # Default score\\n\",\n",
    "    \"                elif key == 'FEEDBACK':\\n\",\n",
    "    \"                    scores['feedback'] = value.strip()\\n\",\n",
    "    \"        \\n\",\n",
    "    \"        return scores\\n\",\n",
    "    \"    except Exception as e:\\n\",\n",
    "    \"        return {\\n\",\n",
    "    \"            'accuracy': 0,\\n\",\n",
    "    \"            'relevance': 0,\\n\",\n",
    "    \"            'completeness': 0,\\n\",\n",
    "    \"            'reasoning': 0,\\n\",\n",
    "    \"            'overall': 0,\\n\",\n",
    "    \"            'feedback': f'Evaluation failed: {str(e)}'\\n\",\n",
    "    \"        }\"\n",
    "   ]\n",
    "  },\n",
    "  {\n",
    "   \"cell_type\": \"code\",\n",
    "   \"execution_count\": null,\n",
    "   \"id\": \"7e0dc154\",\n",
    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# TODO: Create a tool usage evaluator\\n\",\n",
    "    \"def evaluate_tool_usage(messages, expected_tools):\\n\",\n",
    "    \"    \\\"\\\"\\\"Evaluate if the right tools were used\\\"\\\"\\\"\\n\",\n",
    "    \"    # Extract tool names from messages\\n\",\n",
    "    \"    tools_used = []\\n\",\n",
    "    \"    for msg in messages:\\n\",\n",
    "    \"        obj = msg.model_dump()\\n\",\n",
    "    \"        if obj.get(\\\"tool_call_id\\\"):\\n\",\n",
    "    \"            tools_used.append(msg.name)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Calculate metrics\\n\",\n",
    "    \"    tools_used_set = set(tools_used)\\n\",\n",
    "    \"    expected_set = set(expected_tools)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Tools that were expected and used\\n\",\n",
    "    \"    correct_tools = tools_used_set.intersection(expected_set)\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Tools that were expected but not used\\n\",\n",
    "    \"    missing_tools = expected_set - tools_used_set\\n\",\n",
    "    \"    \\n\",\n",
    "    \"    # Extra tools used (not in expected)\\n\",\n",
    "    \"    extra_tools = tools_used_set - expected_set\\n\",\n",
