    "   \"metadata\": {},\n",
    "   \"outputs\": [],\n",
    "   \"source\": [\n",
    "    \"# Run the agent tests\\n\",\n",
    "    \"# For each test case, call the agent and collect the response\\n\",\n",
    "    \"# Store results for evaluation\\n\",\n",
    "    \"\\n\",\n",
    "    \"print(\\\"=== Running Agent Tests ===\\\")\\n\",\n",
    "    \"test_results = []\\n\",\n",
    "    \"\\n\",\n",
    "    \"for i, test_case in enumerate(test_cases):\\n\",\n",
