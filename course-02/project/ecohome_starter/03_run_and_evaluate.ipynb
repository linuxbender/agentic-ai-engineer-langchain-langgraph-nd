    "    \"        \\\"expected_response\\\": \\\"The response should analyze historical usage and provide personalized recommendations\\\",\\n\",\n",
    "    \"    },\\n\",\n",
    "    \"    {\\n\",\n",
    "    \"        \\\"id\\\": \\\"usage_analysis_2\\\",\\n\",\n",
    "    \"        \\\"question\\\": \\\"What was my energy consumption last week and which devices used the most power?\\\",\\n\",\n",
    "    \"        \\\"expected_tools\\\": [\\\"query_energy_usage\\\", \\\"get_recent_energy_summary\\\"],\\n\",\n",
